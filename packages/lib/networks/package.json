{
  "name": "@renproject/contracts",
  "version": "0.3.0",
  "homepage": "https://republicprotocol.github.io/contracts-ts",
  "main": "./lib.js",
  "types": "./lib.d.ts",
  "files": [
    "public/networks",
    "lib.js",
    "lib.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslint-react": "^4.0.0",
    "web3-utils": "^1.0.0-beta.55"
  },
  "scripts": {
    "hash": "echo \"{\\\"HASH\\\": \\\"`git rev-parse HEAD`\\\"}\" > public/commitHash.json",
    "start": "$npm_execpath run hash && react-scripts start",
    "build": "$npm_execpath run hash && react-scripts build",
    "test": "$npm_execpath run hash && react-scripts test --env=jsdom",
    "eject": "$npm_execpath run hash && react-scripts eject",
    "predeploy": "$npm_execpath run hash && $npm_execpath run build",
    "deploy": "$npm_execpath run hash && gh-pages -d build",
    "prepublish": "rm lib.js; rm lib.d.ts; tsc -d --resolveJsonModule lib.ts"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/underscore": "^1.8.18",
    "axios": "^0.19.0",
    "change-case": "^3.1.0",
    "gh-pages": "^2.0.1",
    "immutable": "^3.8.2",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
