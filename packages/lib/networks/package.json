{
  "name": "@renproject/contracts",
  "version": "0.3.27",
  "homepage": "https://renproject.github.io/contracts-ts",
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "darknode-sol": "https://github.com/renproject/darknode-sol#e9e7357e3e8d6e30bbd03a72eaaefdcb9a0e6ad7"
  },
  "scripts": {
    "hash": "echo \"{\\\"HASH\\\": \\\"`git rev-parse HEAD`\\\"}\" > public/commitHash.json",
    "start": "$npm_execpath run hash && react-scripts start",
    "build": "$npm_execpath run hash && react-scripts build",
    "test": "$npm_execpath run hash && react-scripts test --env=jsdom",
    "eject": "$npm_execpath run hash && react-scripts eject",
    "predeploy": "$npm_execpath run hash && $npm_execpath run build",
    "deploy": "$npm_execpath run hash && gh-pages -d build",
    "prepublishOnly": "tsc --module commonjs -d --resolveJsonModule --esModuleInterop --moduleResolution node lib.ts --outDir dist",
    "prepare": "patch-package"
  },
  "devDependencies": {
    "@types/node": "^12.12.11",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/underscore": "^1.9.4",
    "axios": "^0.19.0",
    "change-case": "^3.1.0",
    "gh-pages": "^2.1.1",
    "immutable": "^4.0.0-rc.12",
    "node-sass": "^4.13.0",
    "patch-package": "^6.2.0",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2",
    "web3-utils": "^2.0.0-alpha.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
