{
    "name": "gateway.renproject.io",
    "version": "2.0.0-alpha.0",
    "private": true,
    "homepage": "https://renproject.github.io/gateway-staging.renproject.io/",
    "scripts": {
        "prettier": "yarn fix:prettier",
        "lint": "run-s lint:*",
        "lint:tslint": "tslint --project ./ -t stylish --quiet",
        "lint:prettier": "prettier --check './(src|test)/**/*.ts*'",
        "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
        "fix": "run-s fix:*",
        "fix:tslint": "tslint --fix --project .",
        "fix:prettier": "prettier --write './(src|test)/**/*.ts*'",
        "test": "run-s build:main test:* lint",
        "test:unit": "react-scripts test",
        "start": "rzeact-scripts start",
        "clean": "yarn rimraf ./build",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
        "eject": "react-scripts eject",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d build --repo git@github.com:renproject/gateway-staging.renproject.io.git",
        "link:all": "yarn link @renproject/interfaces @renproject/react-components @renproject/utils @renproject/networks @renproject/ren"
    },
    "dependencies": {
        "@renproject/fonts": "^1.0.1",
        "@renproject/interfaces": "file:../../lib/interfaces",
        "@renproject/react-components": "file:../react-components",
        "@renproject/ren": "file:../../lib/ren",
        "@renproject/utils": "file:../../lib/utils",
        "@sentry/browser": "^5.15.5",
        "@sentry/core": "^5.15.5",
        "@sentry/integrations": "^5.15.5",
        "@types/bchaddrjs": "^0.4.0",
        "@types/bn.js": "^4.11.6",
        "@types/node": "^13.13.4",
        "@types/qrcode.react": "^1.0.0",
        "@types/qs": "^6.9.1",
        "@types/react": "^16.9.34",
        "@types/react-circular-progressbar": "^1.1.0",
        "@types/react-copy-to-clipboard": "^4.3.0",
        "@types/react-dom": "^16.9.7",
        "@types/react-router": "^5.1.7",
        "@types/react-router-dom": "^5.1.5",
        "@types/styled-components": "^5.1.0",
        "axios": "^0.19.2",
        "bchaddrjs": "^0.4.8",
        "bignumber.js": "^9.0.0",
        "bn.js": "^5.1.1",
        "immutable": "^4.0.0-rc.12",
        "localforage": "^1.7.3",
        "polished": "^3.6.0",
        "qrcode.react": "^1.0.0",
        "qs": "^6.9.4",
        "react": "^16.13.1",
        "react-circular-progressbar": "^2.0.3",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "styled-components": "^5.1.0",
        "unstated-next": "^1.1.0",
        "wallet-address-validator": "^0.2.4",
        "web3": "^2.0.0-alpha.1"
    },
    "resolutions": {},
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "tslint": "^6.1.2"
    }
}
