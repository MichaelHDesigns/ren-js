  "license": "GPL-3.0",
  "repository": "renproject/react-components",
  "main": "dist/lib.js",
  "module": "dist/lib.es.js",
  "jsnext:main": "dist/lib.es.js",
  "types": "dist/lib.d.ts",
  "homepage": "https://renproject.github.io/react-components",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "demo:build": "react-scripts build",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "start": "react-scripts start",
    "prepare": "$npm_execpath run build",
    "predeploy": "$npm_execpath run demo:build",
    "deploy": "gh-pages -d build",
    "link:all": "yarn link",
    "unlink:all": "(yarn unlink || true)"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@renproject/fonts": "^1.0.1",
    "@types/react-select": "^3.0.12",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "react-element-to-jsx-string": "^14.3.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "node-sass": "^4.14.1"
  },
  "files": [
    "dist",
    "src/styles"
