{
    "name": "@renproject/ren",
    "version": "0.3.2",
    "description": "Official Ren JavaScript client",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/renproject/ren-js.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "ren",
        "ren-js",
        "renvm",
        "ren project",
        "cryptocurrencies",
        "blockchain",
        "interoperability"
    ],
    "author": "Ren",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/renproject/ren-js/issues"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
        "lint:outdated-imports": "ncu",
        "lint:ts": "tslint --project ./ -t stylish --quiet",
        "lint:extra": "$npm_execpath run --silent lint:unused-exports; $npm_execpath run --silent lint:outdated-imports",
        "lint:all": "$npm_execpath run --silent lint:ts && $npm_execpath run --silent lint:extra",
        "lint": "$npm_execpath run --silent lint:ts",
        "watch": "cross-env NODE_ENV=development webpack --watch --mode development --display-error-details --progress --color",
        "build": "cross-env NODE_ENV=development webpack --mode development --display-error-details --progress --color",
        "build:prod": "$npm_execpath run clean && cross-env NODE_ENV=production webpack --mode production --progress --color",
        "test": "nyc ./node_modules/ts-mocha/bin/ts-mocha --bail --sort --exit --timeout 180000 --paths -p ./tsconfig.json ./test/*.spec.ts ./test/**/*.spec.ts --ignore ./test/testutils/chai.d.ts",
        "clean": "rm -rf dist",
        "prepublishOnly": "npmignore && $npm_execpath run lint && $npm_execpath run build:prod"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
        "@types/bn.js": "^4.11.5",
        "@types/chai": "^4.2.7",
        "@types/chai-as-promised": "^7.1.2",
        "@types/detect-node": "^2.0.0",
        "@types/expand-tilde": "^2.0.0",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^5.2.7",
        "@types/node-fetch": "^2.5.4",
        "@types/node-persist": "3.0.0",
        "@types/node-rsa": "1.0.0",
        "@types/underscore": "^1.9.4",
        "babel-loader": "8.0.6",
        "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
        "babel-polyfill": "6.26.0",
        "bitgo-utxo-lib": "^1.7.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-bignumber": "noiach/chai-bignumber#fd75e47",
        "chalk": "^3.0.0",
        "cross-env": "6.0.3",
        "dotenv": "^8.2.0",
        "mocha": "^6.2.2",
        "npm-check-updates": "^4.0.1",
        "npmignore": "^0.2.0",
        "nyc": "^15.0.0",
        "qrcode-terminal": "^0.12.0",
        "truffle-hdwallet-provider": "1.0.17",
        "ts-loader": "^6.2.1",
        "ts-mocha": "^6.0.0",
        "ts-unused-exports": "^5.1.0",
        "tslint": "5.20.1",
        "tslint-config-prettier": "1.18.0",
        "tslint-config-standard": "9.0.0",
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "^6.2.0",
        "tslint-react": "^4.1.0",
        "typedoc": "^0.15.5",
        "typedoc-webpack-plugin": "1.1.4",
        "typescript": "3.7.4",
        "webpack": "4.41.4",
        "webpack-cli": "3.3.10",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "@renproject/contracts": "^0.3.28",
        "@types/bn.js": "^4.11.5",
        "@types/bs58": "^4.0.1",
        "@types/node": "^12.12.21",
        "axios": "^0.19.0",
        "bignumber.js": "^9.0.0",
        "bitcore-lib": "^8.13.2",
        "bitcore-lib-cash": "^8.13.2",
        "bitcore-lib-zcash": "zcash-hackworks/bitcore-lib-zcash",
        "bn.js": "^5.0.0",
        "bs58": "^4.0.1",
        "ethereumjs-util": "^6.2.0",
        "immutable": "^4.0.0-rc.12",
        "send-crypto": "0.1.9",
        "sha3": "^2.1.1",
        "wallet-address-validator": "^0.2.4",
        "web3": "^2.0.0-alpha.1",
        "web3-core": "^2.0.0-alpha.1",
        "web3-eth-abi": "^2.0.0-alpha.1"
    },
    "resolutions": {
        "sha3": "^2.1.1",
        "scrypt": "https://github.com/ren-forks/node-scrypt"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "include": [
            "src"
        ]
    }
}
